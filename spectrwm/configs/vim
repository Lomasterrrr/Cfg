syntax		on
set		background=dark
set		cursorline
set		noswapfile
set		paste

hi Normal	ctermbg=NONE guibg=NONE
hi Type		ctermfg=green cterm=bold
hi PreProc	ctermfg=blue cterm=bold
hi Constant	ctermfg=magenta cterm=bold
hi Number	ctermfg=magenta cterm=bold
hi String	ctermfg=magenta cterm=bold
hi Identifier	ctermfg=14
hi Function	ctermfg=6
hi Statement	ctermfg=3
hi Special	ctermfg=red
hi Comment	ctermfg=cyan cterm=bold

hi CursorLine	cterm=bold ctermbg=233 ctermfg=NONE
